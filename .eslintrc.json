{
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "ecmaFeatures": {
      "modules": true,
      "jsx": true
    },
    "extends": "airbnb",
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "global-require": 0,

      "no-use-before-define": 0,
      "linebreak-style": 0
    }
  }